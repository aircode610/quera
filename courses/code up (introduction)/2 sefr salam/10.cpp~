#include <bits/stdc++.h>
#define int long long

using namespace std;

int32_t main() {
  string dna;
  cin >> dna;
  for (int i = 0; i < dna.size(); i++){
    if (dna[i] == 'A')
      a.push_back(i);
    else if (dna[i] == 'G')
      g.push_back(i);
    else if (dna[i] == 'C')
      c.push_back(i);
  }
  return 0;
}

